.videoplayer{position:absolute;display:none;overflow: hidden}
.videoplayer div {
  line-height: 0;
}
.video-container.played #large_click_area{
  display: none !important;
}
.videoplayer #buffer_wrapper {}
.videoplayer #buffer_wrapper #buffer_indicator {}
.videoplayer #timer_wrapper {
  display: none;
}
.videoplayer #timer_wrapper #timer_duration {}
.videoplayer #timer_wrapper #timer_remaining {}
.videoplayer #timer_wrapper #timer_current {}
.videoplayer:hover #large_toggle_button {
  opacity: 1;
}
.videoplayer #large_toggle_button {
  width: 60px;
  height: 60px;
  margin: auto;
  top: 0; left: 0; bottom: 0; right: 0;
  opacity: 0.6;
  pointer-events: none;
  background-size: cover;
  background-image: url('data:image/png;base64,');
}
.videoplayer #large_toggle_button.playing {
  display: none;
}
.videoplayer #controls_wrapper {
  bottom: 0px;
  left: 0px;
  width: 100%;
  background-color: rgba(0,0,0,0.3);
  padding: 8px;
  opacity: 0;
  -webkit-transition-property: opacity;
  transition-property: opacity;
  -webkit-transition-duration: .4s;
  transition-duration: .4s;
}
.videoplayer:hover #controls_wrapper {
  opacity: 1;
}
.videoplayer #controls_wrapper > div {
  position: relative;
}
.videoplayer #toggle_button,
.videoplayer #volume_button {
  width: 16px;
  height: 16px;
  background-size: cover;
}
.videoplayer #toggle_button {
  background-image: url('data:image/png;base64,');
}
.videoplayer #toggle_button.playing {
  background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQBAMAAADt3eJSAAAAJ1BMVEUAAAC3G0DUdYvbjaC3G0C3G0C3G0C3G0C3G0DUdYvbjaC3G0CzDTRJvxP0AAAAC3RSTlMAwZuA1vvX076ZfeQbHwoAAAAkSURBVAjXY+A6Gs7A0BozgYH59DYGBuk9CkDGFnox4JbCnQEAOqEn0U4mZAkAAAAASUVORK5CYII=');
}
.videoplayer #volume_button {
  background-image: url('data:image/png;base64,');
}
.videoplayer #volume_button.muted {
  background-image: url('data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAMAAAAoLQ9TAAAAM1BMVEUAAADHTmu3G0C3G0C3G0C3G0C3G0Drvsi3G0CzDjW3G0DERGPERWTx0djuydG3G0C0ETdTFZAoAAAAD3RSTlMAxjfuz5lfQTksHtXTDg70CVK1AAAAPElEQVQY02PACXjZUfmcLDwofDZmAUYok4uJiYmDlZ8fLsDMDwYIAX7KBRCGYlqL4TB0p3OjeY6PHYevAVCkBEVrM9WRAAAAAElFTkSuQmCC');
}
.videoplayer #volume_button {
  position: absolute !important;
  right: 62px;
}
.videoplayer #progress_bar,
.videoplayer #volume_bar {
  height: 10px;
  background-color: rgba(255,255,255,0.5);
  margin: 3px 6px;
}
.videoplayer #progress_bar {
  width: 100px;
}
.videoplayer #volume_bar {
  position: absolute !important;
  right: 0;
  width: 50px;
}
.videoplayer #progress_bar div,
.videoplayer #volume_bar div {
  height: 100%;
  width: 100%;
}
.videoplayer #progress_current,
.videoplayer #volume_current {
  background-color: #B71B40;
}
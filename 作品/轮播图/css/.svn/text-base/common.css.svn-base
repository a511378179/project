section[data-name='Scene 1 - portrait'] {
  display: none !important;
}
section[data-name='Scene 1 - landscape'] {
  margin: 0 !important;
  width: 100% !important;
  height: 100% !important;
  -webkit-transform: scale(1) !important;
  -moz-transform: scale(1) !important;
  -o-transform: scale(1) !important;
  -ms-transform: scale(1) !important;
  transform: scale(1) !important;
  display: block !important;
  font-size: 0;
}
section[data-name='Scene 1 - landscape'] img {
  display: block;
  width: 100%;
}
section[data-name='Scene 1 - landscape'] .video-container {
  position: relative;
  width: 100%;
  display: block;
  overflow: hidden;
}
section[data-name='Scene 1 - landscape'] .video-container .videoplayer {
  position: absolute;
  width: 100.2% !important;
  height: auto !important;
  left: -1px !important;
  top: 0 !important;
  z-index: auto !important;
}
section[data-name='Scene 1 - landscape'] .video-container .videoplayer video {
  width: 100%;
  height: auto;
}
section[data-name='Scene 1 - landscape'] .video-container .videoplayer #large_click_area {
  background: rgba(255, 255, 255, 0.001);
}
section[data-name='Scene 1 - landscape'] div[data-type="videosequence"] {
  position: absolute;
  width: 100% !important;
  height: 100% !important;
  left: 0 !important;
  top: 0 !important;
  z-index: auto !important;
}
section[data-name='Scene 1 - landscape'] div[data-type="videosequence"] #large_click_area {
  background: rgba(255, 255, 255, 0.001);
}
section[data-name='Scene 1 - landscape'] div[data-type="videosequence"] .player-container {
  position: absolute;
  width: 100%;
  height: auto;
}
section[data-name='Scene 1 - landscape'] div[data-type="videosequence"] .player-container > div {
  position: relative;
  width: 100% !important;
  height: auto !important;
}
section[data-name='Scene 1 - landscape'] div[data-type="videosequence"] .player-container .videoview {
  position: relative;
  width: 100% !important;
  height: auto !important;
}
section[data-name='Scene 1 - landscape'] .placeholder {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
  text-align: center;
  display: none;
  font-size: 0;
  z-index: 1000;
}
section[data-name='Scene 1 - landscape'] .placeholder:after {
  content: '';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
section[data-name='Scene 1 - landscape'] .placeholder .plzrotate {
  position: relative;
  display: inline-block;
  text-align: center;
  vertical-align: middle;
}
section[data-name='Scene 1 - landscape'] .placeholder .pl-logo {
  position: relative;
  margin: 0 auto;
}
section[data-name='Scene 1 - landscape'] .placeholder .text {
  position: relative;
  margin-top: 12px;
  font-size: 12px;
  text-align: center;
}
section[data-name='Scene 1 - landscape'] .placeholder .spinner {
  position: relative;
  width: 17px;
  margin: 0 auto;
  margin-top: 15px;
  -webkit-animation: rotating 2s cubic-bezier(0.77, 0, 0.175, 1) 1s infinite alternate;
  -moz-animation: rotating 2s cubic-bezier(0.77, 0, 0.175, 1) 1s infinite alternate;
  -o-animation: rotating 2s cubic-bezier(0.77, 0, 0.175, 1) 1s infinite alternate;
  animation: rotating 2s cubic-bezier(0.77, 0, 0.175, 1) 1s infinite alternate;
}
@media screen and (min-width: 480px) and (max-height: 499px) and (orientation: landscape) {
  .mobile .placeholder {
    display: block !important;
  }
}
.btn-container {
  position: relative;
}
.btn-container .normal {
  position: relative;
  width: 100%;
  display: block;
}
.btn-container .hover {
  position: absolute;
  width: 100%;
  opacity: 0;
  top: 0;
  display: block;
}
@-webkit-keyframes rotating {
  0% {
    -webkit-transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(90deg);
  }
}
@-moz-keyframes rotating {
  0% {
    -moz-transform: rotate(0deg);
  }
  100% {
    -moz-transform: rotate(90deg);
  }
}
@-o-keyframes rotating {
  0% {
    -o-transform: rotate(0deg);
  }
  100% {
    -o-transform: rotate(90deg);
  }
}
@keyframes rotating {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(90deg);
  }
}
